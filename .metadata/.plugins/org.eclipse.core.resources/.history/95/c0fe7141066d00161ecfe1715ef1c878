#include <cstdio>
#include <cstdlib>
#include <cstring>

#include "spImageProc.h"
extern "C"{
#include "SPConfig.h"
#include "SPPoint.h"
#include "MainAux.h"
}

#define BUFSIZE 1024
#define DEFCON "spcbir.config"

int main(int argc, const char* argv){
	SPConfig config;
	const char* filename = (char*)malloc(BUFSIZE);
	for(int i=0; i<argc;i++){
		if(strcmp(argv[i],"-c")==0){
			if(i+1<argc)
				filename=argv[i+1];
		}
	}
}


